package com.cbs.dto.report;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 *
 * @author Sudhir
 */
public class LoanRecoveryTable implements Serializable {

    private String accountNumber;
    private String acDesc;
    private String name;
    private float roi;
    private BigDecimal amountSanc;
    private BigDecimal amtDis;
    private BigDecimal amtCash;
    private BigDecimal amtClearing;
    private BigDecimal amtClearingdr;
    private BigDecimal amtTransfer;
    private BigDecimal npaRec;
    private BigDecimal intRec;
    private BigDecimal prinRec;
    private BigDecimal balance;
    private String recoverType;

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public float getRoi() {
        return roi;
    }

    public void setRoi(float roi) {
        this.roi = roi;
    }

    public BigDecimal getBalance() {
        return balance;
    }

    public void setBalance(BigDecimal balance) {
        this.balance = balance;
    }

    public String getAccountNumber() {
        return accountNumber;
    }

    public void setAccountNumber(String accountNumber) {
        this.accountNumber = accountNumber;
    }

    public BigDecimal getAmountSanc() {
        return amountSanc;
    }

    public void setAmountSanc(BigDecimal amountSanc) {
        this.amountSanc = amountSanc;
    }

    public BigDecimal getAmtCash() {
        return amtCash;
    }

    public void setAmtCash(BigDecimal amtCash) {
        this.amtCash = amtCash;
    }

    public BigDecimal getAmtClearing() {
        return amtClearing;
    }

    public void setAmtClearing(BigDecimal amtClearing) {
        this.amtClearing = amtClearing;
    }

    public BigDecimal getAmtClearingdr() {
        return amtClearingdr;
    }

    public void setAmtClearingdr(BigDecimal amtClearingdr) {
        this.amtClearingdr = amtClearingdr;
    }

    public BigDecimal getAmtDis() {
        return amtDis;
    }

    public void setAmtDis(BigDecimal amtDis) {
        this.amtDis = amtDis;
    }

    public BigDecimal getAmtTransfer() {
        return amtTransfer;
    }

    public void setAmtTransfer(BigDecimal amtTransfer) {
        this.amtTransfer = amtTransfer;
    }

    public String getAcDesc() {
        return acDesc;
    }

    public void setAcDesc(String acDesc) {
        this.acDesc = acDesc;
    }

    public BigDecimal getNpaRec() {
        return npaRec;
    }

    public void setNpaRec(BigDecimal npaRec) {
        this.npaRec = npaRec;
    }

    public BigDecimal getIntRec() {
        return intRec;
    }

    public void setIntRec(BigDecimal intRec) {
        this.intRec = intRec;
    }

    public BigDecimal getPrinRec() {
        return prinRec;
    }

    public void setPrinRec(BigDecimal prinRec) {
        this.prinRec = prinRec;
    }

    public String getRecoverType() {
        return recoverType;
    }

    public void setRecoverType(String recoverType) {
        this.recoverType = recoverType;
    }
    
}
