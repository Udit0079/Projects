package com.hrms.common.to;

import java.io.Serializable;
import java.util.Date;

/**
 *
 * @author Zeeshan Waris
 */

public class LeaveMasterTO implements Serializable {

    private static final long serialVersionUID = 1L;

    protected LeaveMasterPKTO leaveMasterPKTO;

    private int numDays;

    private String applicableDate;

    private String description;

    private char encash;

    private Integer defaultComp;

    private String statFlag;

    private String statUpFlag;

    private Date modDate;

    private String authBy;

    private String enteredBy;

    private String leaveNature;
    
    private HrMstStructTO structMasterTO;

    public String getApplicableDate() {
        return applicableDate;
    }

    public void setApplicableDate(String applicableDate) {
        this.applicableDate = applicableDate;
    }

    public String getAuthBy() {
        return authBy;
    }

    public void setAuthBy(String authBy) {
        this.authBy = authBy;
    }

    public Integer getDefaultComp() {
        return defaultComp;
    }

    public void setDefaultComp(Integer defaultComp) {
        this.defaultComp = defaultComp;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }

    public char getEncash() {
        return encash;
    }

    public void setEncash(char encash) {
        this.encash = encash;
    }

    public String getEnteredBy() {
        return enteredBy;
    }

    public void setEnteredBy(String enteredBy) {
        this.enteredBy = enteredBy;
    }

    public LeaveMasterPKTO getLeaveMasterPKTO() {
        return leaveMasterPKTO;
    }

    public void setLeaveMasterPKTO(LeaveMasterPKTO leaveMasterPKTO) {
        this.leaveMasterPKTO = leaveMasterPKTO;
    }

    public Date getModDate() {
        return modDate;
    }

    public void setModDate(Date modDate) {
        this.modDate = modDate;
    }

    public int getNumDays() {
        return numDays;
    }

    public void setNumDays(int numDays) {
        this.numDays = numDays;
    }

    public String getStatFlag() {
        return statFlag;
    }

    public void setStatFlag(String statFlag) {
        this.statFlag = statFlag;
    }

    public String getStatUpFlag() {
        return statUpFlag;
    }

    public void setStatUpFlag(String statUpFlag) {
        this.statUpFlag = statUpFlag;
    }

    public HrMstStructTO getStructMasterTO() {
        return structMasterTO;
    }

    public void setStructMasterTO(HrMstStructTO structMasterTO) {
        this.structMasterTO = structMasterTO;
    }

    public String getLeaveNature() {
        return leaveNature;
    }

    public void setLeaveNature(String leaveNature) {
        this.leaveNature = leaveNature;
    }
}
