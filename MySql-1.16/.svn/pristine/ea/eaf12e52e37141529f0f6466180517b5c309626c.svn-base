package com.cbs.pojo;

import java.math.BigDecimal;

/**
 *
 * @author root
 */
public class NpaCategoryReport {

    private String acno;
    private String custName;
    private String type;
    private String sanctionLimitDt;
    private BigDecimal sanctionLimit;
    private BigDecimal amount;
    private BigDecimal installAmt;
    private String dt;
    private String remarks;
    private BigDecimal matValue;
    private String status;
    private String secUnSec;
    private BigDecimal unSecAmt;
    private BigDecimal unSecSecAmt;
    private String description;

    public String getAcno() {
        return acno;
    }

    public void setAcno(String acno) {
        this.acno = acno;
    }

    public BigDecimal getAmount() {
        return amount;
    }

    public void setAmount(BigDecimal amount) {
        this.amount = amount;
    }

    public String getCustName() {
        return custName;
    }

    public void setCustName(String custName) {
        this.custName = custName;
    }

    public String getDt() {
        return dt;
    }

    public void setDt(String dt) {
        this.dt = dt;
    }

    public BigDecimal getInstallAmt() {
        return installAmt;
    }

    public void setInstallAmt(BigDecimal installAmt) {
        this.installAmt = installAmt;
    }

    public BigDecimal getMatValue() {
        return matValue;
    }

    public void setMatValue(BigDecimal matValue) {
        this.matValue = matValue;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks;
    }

    public BigDecimal getSanctionLimit() {
        return sanctionLimit;
    }

    public void setSanctionLimit(BigDecimal sanctionLimit) {
        this.sanctionLimit = sanctionLimit;
    }

    public String getSanctionLimitDt() {
        return sanctionLimitDt;
    }

    public void setSanctionLimitDt(String sanctionLimitDt) {
        this.sanctionLimitDt = sanctionLimitDt;
    }

    public String getSecUnSec() {
        return secUnSec;
    }

    public void setSecUnSec(String secUnSec) {
        this.secUnSec = secUnSec;
    }

    public String getStatus() {
        return status;
    }

    public void setStatus(String status) {
        this.status = status;
    }

    public String getType() {
        return type;
    }

    public void setType(String type) {
        this.type = type;
    }

    public BigDecimal getUnSecAmt() {
        return unSecAmt;
    }

    public void setUnSecAmt(BigDecimal unSecAmt) {
        this.unSecAmt = unSecAmt;
    }

    public BigDecimal getUnSecSecAmt() {
        return unSecSecAmt;
    }

    public void setUnSecSecAmt(BigDecimal unSecSecAmt) {
        this.unSecSecAmt = unSecSecAmt;
    }

    public String getDescription() {
        return description;
    }

    public void setDescription(String description) {
        this.description = description;
    }
}
