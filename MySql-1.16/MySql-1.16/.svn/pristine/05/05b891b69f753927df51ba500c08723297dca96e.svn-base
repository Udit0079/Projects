package com.cbs.dto.report;

import java.math.BigDecimal;

/**
 *
 * @author root
 */
public class FormulaReturnOfLoanAdv {
    
    BigDecimal loanAmt;
    BigDecimal odCcAmt;
    BigDecimal billPurAmt;

    public BigDecimal getBillPurAmt() {
        return billPurAmt;
    }

    public void setBillPurAmt(BigDecimal billPurAmt) {
        this.billPurAmt = billPurAmt;
    }

    public BigDecimal getLoanAmt() {
        return loanAmt;
    }

    public void setLoanAmt(BigDecimal loanAmt) {
        this.loanAmt = loanAmt;
    }

    public BigDecimal getOdCcAmt() {
        return odCcAmt;
    }

    public void setOdCcAmt(BigDecimal odCcAmt) {
        this.odCcAmt = odCcAmt;
    }
    
}
