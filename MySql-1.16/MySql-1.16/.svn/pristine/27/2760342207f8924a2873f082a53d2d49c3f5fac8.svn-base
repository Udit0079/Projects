package com.cbs.web.pojo.neftrtgs;

/**
 *
 * @author root
 */
public class Block4Items {

    private String blockBeginIdentifier;
    private String transactionReferenceNo;
    private String valueDateCurrencyAndAmount;
    private String orderingCustomer;
    private String beneficiaryCustomer;
    //private String remittanceInformation;
    private String blockEndIdentifier;

    public String getBeneficiaryCustomer() {
        return beneficiaryCustomer;
    }

    public void setBeneficiaryCustomer(String beneficiaryCustomer) {
        this.beneficiaryCustomer = beneficiaryCustomer;
    }

    public String getBlockBeginIdentifier() {
        return blockBeginIdentifier;
    }

    public void setBlockBeginIdentifier(String blockBeginIdentifier) {
        this.blockBeginIdentifier = blockBeginIdentifier;
    }

    public String getBlockEndIdentifier() {
        return blockEndIdentifier;
    }

    public void setBlockEndIdentifier(String blockEndIdentifier) {
        this.blockEndIdentifier = blockEndIdentifier;
    }

    public String getOrderingCustomer() {
        return orderingCustomer;
    }

    public void setOrderingCustomer(String orderingCustomer) {
        this.orderingCustomer = orderingCustomer;
    }

//    public String getRemittanceInformation() {
//        return remittanceInformation;
//    }
//
//    public void setRemittanceInformation(String remittanceInformation) {
//        this.remittanceInformation = remittanceInformation;
//    }
    public String getTransactionReferenceNo() {
        return transactionReferenceNo;
    }

    public void setTransactionReferenceNo(String transactionReferenceNo) {
        this.transactionReferenceNo = transactionReferenceNo;
    }

    public String getValueDateCurrencyAndAmount() {
        return valueDateCurrencyAndAmount;
    }

    public void setValueDateCurrencyAndAmount(String valueDateCurrencyAndAmount) {
        this.valueDateCurrencyAndAmount = valueDateCurrencyAndAmount;
    }

    public Block4Items() {
    }

    public Block4Items(String blockBeginIdentifier, String transactionReferenceNo, String valueDateCurrencyAndAmount, String sendingCustomerAcType, String sendingCustomerAccount, String sendingCustomerAccountName, String beneficiaryCustomerAcType, String beneficiaryCustomerAccount, String beneficiaryCustomerAccountName, String orderingCustomer, String beneficiaryCustomer, String blockEndIdentifier) {
        this.blockBeginIdentifier = blockBeginIdentifier;
        this.transactionReferenceNo = transactionReferenceNo;
        this.valueDateCurrencyAndAmount = valueDateCurrencyAndAmount;
        //this.remittanceInformation = remittanceInformation;
        this.orderingCustomer = orderingCustomer;
        this.beneficiaryCustomer = beneficiaryCustomer;
        this.blockEndIdentifier = blockEndIdentifier;
    }

    @Override
    public String toString() {
        return (blockBeginIdentifier + transactionReferenceNo + valueDateCurrencyAndAmount + orderingCustomer + beneficiaryCustomer + blockEndIdentifier);
    }
}
