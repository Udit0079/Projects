package com.cbs.dto.report;

import java.io.Serializable;
import java.math.BigDecimal;

/**
 *
 * @author SAMY
 */
public class LoanIntDetailsMonthWisePojo implements Serializable {
    private String acNo;
    private String custName;
    private String acType;
    private BigDecimal sanctionAmt;
    private String sanctionDate;
    private BigDecimal dpLimit;
    private BigDecimal outstanding;
    private String dt;
    private BigDecimal intDrAmt;
    private BigDecimal intCrAmt;
    private BigDecimal diffAmt;
    private String drCrInd;
    private String schemeType;

    public String getAcNo() {
        return acNo;
    }
    public void setAcNo(String acNo) {
        this.acNo = acNo;
    }
    public String getAcType() {
        return acType;
    }
    public void setAcType(String acType) {
        this.acType = acType;
    }
    public String getCustName() {
        return custName;
    }
    public void setCustName(String custName) {
        this.custName = custName;
    }
    public BigDecimal getDpLimit() {
        return dpLimit;
    }
    public void setDpLimit(BigDecimal dpLimit) {
        this.dpLimit = dpLimit;
    }
    public String getDt() {
        return dt;
    }
    public void setDt(String dt) {
        this.dt = dt;
    }
    public BigDecimal getIntCrAmt() {
        return intCrAmt;
    }
    public void setIntCrAmt(BigDecimal intCrAmt) {
        this.intCrAmt = intCrAmt;
    }
    public BigDecimal getIntDrAmt() {
        return intDrAmt;
    }
    public void setIntDrAmt(BigDecimal intDrAmt) {
        this.intDrAmt = intDrAmt;
    }
    public BigDecimal getOutstanding() {
        return outstanding;
    }
    public void setOutstanding(BigDecimal outstanding) {
        this.outstanding = outstanding;
    }
    public BigDecimal getSanctionAmt() {
        return sanctionAmt;
    }
    public void setSanctionAmt(BigDecimal sanctionAmt) {
        this.sanctionAmt = sanctionAmt;
    }
    public String getSanctionDate() {
        return sanctionDate;
    }
    public void setSanctionDate(String sanctionDate) {
        this.sanctionDate = sanctionDate;
    }
    public BigDecimal getDiffAmt() {
        return diffAmt;
    }
    public void setDiffAmt(BigDecimal diffAmt) {
        this.diffAmt = diffAmt;
    }
    public String getDrCrInd() {
        return drCrInd;
    }
    public void setDrCrInd(String drCrInd) {
        this.drCrInd = drCrInd;
    }    
    public String getSchemeType() {
        return schemeType;
    }
    public void setSchemeType(String schemeType) {
        this.schemeType = schemeType;
    }    
}
