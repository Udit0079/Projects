        return closingPrinciple;
    }

    public void setClosingPrinciple(BigDecimal closingPrinciple) {
        this.closingPrinciple = closingPrinciple;
    }

    public BigDecimal getOpeningPrinciple() {
        return openingPrinciple;
    }

    public void setOpeningPrinciple(BigDecimal openingPrinciple) {
        this.openingPrinciple = openingPrinciple;
    }
    
      
