(select round((sum(ifnull(cramt,0)) - sum(ifnull(dramt,0))),2) from ca_recon where auth= 'Y' and acno = r.acno and dt <= '20170725' and auth = 'Y') as closingBal, 
(select round((sum(ifnull(cramt,0)) - sum(ifnull(dramt,0))),2) from ca_recon where auth= 'Y' and acno = r.acno and dt <= r.dt and auth = 'Y') as balance, 
