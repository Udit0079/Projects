from customerid ci, td_accountmaster am, td_vouchmst v   
where  ci.acno = am.acno and am.acno = v.acno and substring(am.acno,3,2) in (select acctcode from accounttypemaster where acctnature in ('FD','MS')) 
and (am.ClosingDate is null or am.ClosingDate = '' or am.ClosingDate>'20170401')  
and (date_format(v.ClDt,'%Y%m%d') is null or date_format(v.ClDt,'%Y%m%d')  = '' or date_format(v.ClDt,'%Y%m%d') >'20170401')  
and ci.custid =  '42310' group by ci.CustId, ci.Acno,v.VoucherNo ) aa, accounttypemaster atm, branchmaster bm, bnkadd ba, parameterinfo pi  
