package com.cbs.pojo;

import java.math.BigDecimal;

/**
 *
 * @author root
 */
public class FinancialSalProjectionPojo {
    public String month;
    public String componentType;
    public BigDecimal componentAmt ;
    public String purposeCode;
    public String purposeType;

    public String getPurposeType() {
        return purposeType;
    }

    public void setPurposeType(String purposeType) {
        this.purposeType = purposeType;
    }
    
    public String getMonth() {
        return month;
    }

    public void setMonth(String month) {
        this.month = month;
    }

    public String getComponentType() {
        return componentType;
    }

    public void setComponentType(String componentType) {
        this.componentType = componentType;
    }

    public BigDecimal getComponentAmt() {
        return componentAmt;
    }

    public void setComponentAmt(BigDecimal componentAmt) {
        this.componentAmt = componentAmt;
    }

    public String getPurposeCode() {
        return purposeCode;
    }

    public void setPurposeCode(String purposeCode) {
        this.purposeCode = purposeCode;
    }

}
