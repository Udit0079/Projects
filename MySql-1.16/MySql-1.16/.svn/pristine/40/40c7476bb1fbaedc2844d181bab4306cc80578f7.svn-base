package com.cbs.pojo;

import java.math.BigDecimal;

/**
 *
 * @author root
 */
public class NpaRecoveryPojo {

    private String acno;
    private BigDecimal balance;
    private BigDecimal subRec;
    private BigDecimal deRec;
    private BigDecimal lossRec;
    private String npaStatus;
    private String name;
    private String acType;
    private String acName;
    private String branchName;
    private BigDecimal loanRec;
    private BigDecimal totalLoanRec;
    private BigDecimal totalNpaRec;

    public String getAcno() {
        return acno;
    }

    public void setAcno(String acno) {
        this.acno = acno;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getNpaStatus() {
        return npaStatus;
    }

    public void setNpaStatus(String npaStatus) {
        this.npaStatus = npaStatus;
    }

    public String getAcName() {
        return acName;
    }

    public void setAcName(String acName) {
        this.acName = acName;
    }

    public String getAcType() {
        return acType;
    }

    public void setAcType(String acType) {
        this.acType = acType;
    }

    public String getBranchName() {
        return branchName;
    }

    public void setBranchName(String branchName) {
        this.branchName = branchName;
    }

    public BigDecimal getBalance() {
        return balance;
    }

    public void setBalance(BigDecimal balance) {
        this.balance = balance;
    }

    public BigDecimal getDeRec() {
        return deRec;
    }

    public void setDeRec(BigDecimal deRec) {
        this.deRec = deRec;
    }

    public BigDecimal getLossRec() {
        return lossRec;
    }

    public void setLossRec(BigDecimal lossRec) {
        this.lossRec = lossRec;
    }

    public BigDecimal getSubRec() {
        return subRec;
    }

    public void setSubRec(BigDecimal subRec) {
        this.subRec = subRec;
    }
    public BigDecimal getLoanRec() {
        return loanRec;
    }
    public void setLoanRec(BigDecimal loanRec) {
        this.loanRec = loanRec;
    }

    public BigDecimal getTotalLoanRec() {
        return totalLoanRec;
    }

    public void setTotalLoanRec(BigDecimal totalLoanRec) {
        this.totalLoanRec = totalLoanRec;
    }

    public BigDecimal getTotalNpaRec() {
        return totalNpaRec;
    }

    public void setTotalNpaRec(BigDecimal totalNpaRec) {
        this.totalNpaRec = totalNpaRec;
    }
    
}
